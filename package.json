{
  "name": "rs-cmp",
  "version": "1.0.0",
  "description": "GDPR-compliant Consent Management Platform",
  "main": "dist/cmp.js",
  "scripts": {
    "build:sdk": "npm run build:sdk:dev && npm run build:sdk:prod",
    "build:sdk:dev": "esbuild src/sdk/index.ts --bundle --outfile=dist/cmp.js --format=iife --global-name=RSCMP --target=es2015",
    "build:sdk:prod": "esbuild src/sdk/index.ts --bundle --minify --outfile=dist/cmp.min.js --format=iife --global-name=RSCMP --target=es2015",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build": "npm run build:sdk && npm run build:backend",
    "dev:backend": "tsx watch src/backend/server.ts",
    "start": "node dist/backend/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js"
  },
  "keywords": [
    "cmp",
    "gdpr",
    "consent",
    "cookie",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/rate-limit": "^9.0.1",
    "fastify": "^4.25.0",
    "pg": "^8.11.3"
  }
}
